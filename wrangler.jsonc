{
  "name": "michelle-portfolio",
  "main": "src/index.js",
  "compatibility_date": "2025-12-29",
  "compatibility_flags": [
    "enable_nodejs_process_v2",
    "enable_nodejs_http_server_modules",
    "enable_nodejs_http_modules"
  ],
  "assets": {
    "directory": "."
  },
  "placement": {
    "mode": "smart"
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": true,
      "persist": true,
      "head_sampling_rate": 1
    }
  }
}